    //// Click on my picture
    
    let clicks = 0
    
  $(document).ready(function(){
    $("#mepic").click(function(){
      clicks += 1
      if(clicks %2 == 0){
      // code for even number (going back)
      $("#mepic").animate({ width: "20vh",
      });
      document.getElementById("mepic").style.filter = "grayscale(100%)"
      $("#textoutput").slideUp(500)
      $("#mepic3").toggle(500) // geting rid od the second pic
      $("#textoutput").slideDown(1000)

      // delaying the load for the textoutput to be smooth 
      let delay = 1300
      setTimeout(function(){
        //$("#textoutput").load("second")
        window.location.reload(); // only solution for js to work after ajax fired 
      },delay)
      

      }else{
      // for odd numbers
      $("#mepic").animate({ width: "30vh",
      });
      $("#textoutput").slideUp(500)
      $("#textoutput").slideDown(500)
      document.getElementById("mepic").style.filter = "grayscale(0%)"
      //document.getElementById("container").style.justifyContent = "center"
      $("#textoutput").load("aboutmestory")
      $("#mepic3").toggle(1000)
  }
  });
  // what I believe in
});

